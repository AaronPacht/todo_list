<h1>Create a Todo</h1>
<form action="/create" method="post">
    <div>
        <label for="name">Todo: </label><br>
        <input name="name" id="name" type="text">
    </div>
    <div>
        <label for="place">Location: </label><br>
        <input name="place" id="place" type="text">
    </div>
    <div>
        <label for="note">Note: </label><br>
        <input name="note" id="note" type="text">
    </div>
    <input id="submit" type="submit" value="Create Todo">
</form>